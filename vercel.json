{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "@php-lambda/vercel"
        },
        {
            "src": "RSSEIMundo.php",
            "use": "@php-lambda/vercel"
        },
        {
            "src": "RSSEIPais.php",
            "use": "@php-lambda/vercel"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}