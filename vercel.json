{
    "version": 2,
    "builds": [
        {
            "src": "api/*.php",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        }
    ]
}